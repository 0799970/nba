#!/usr/bin/env node

var fs = require( "fs" );
var path = require( "path" );

var get = require( "../lib/get" );
var maps = require( "../lib/maps" );
var util = require( "../lib/util" );
var getTeams = require( "../lib/get-teams" );

getTeams().then( function ( teamData ) {
  var filePath = path.join( __dirname, "../data/teams.json" );
  fs.writeFileSync( filePath, JSON.stringify( teamData ) );
});
